{
  "name": "iobroker.test-react",
  "version": "0.0.1",
  "description": "test react",
  "author": {
    "name": "Swissglider",
    "email": "npm@swissglider.com"
  },
  "homepage": "https://github.com/swissglider/ioBroker.test-react",
  "license": "MIT",
  "keywords": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swissglider/ioBroker.test-react"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.4.0",
    "@material-ui/core": "^4.11.2",
    "@types/react-router-dom": "^5.1.6",
    "grommet": "^2.16.0",
    "grommet-icons": "^4.5.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-react-jsx-source": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@iobroker/adapter-react": "^1.4.11",
    "@iobroker/testing": "^2.4.0",
    "@material-ui/icons": "^4.11.2",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/gulp": "^4.0.7",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/sinon": "^9.0.9",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "gulp": "^4.0.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^4.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "deployToServer": "./scripts/deploy.sh"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/swissglider/ioBroker.test-react/issues"
  },
  "readmeFilename": "README.md"
}
